@model IEnumerable<Analyst.Domain.Edgar.SIC>
@{
    Layout = "~/Views/Edgar/_EdgarLayout.cshtml";
}


<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>SICs</title>
</head>
<body>
    <div>
        <table border="1" cellpadding="10" cellspacing="0">
            <thead>
                <tr>
                    <th>Code</th>
                    <th>ADOffice</th>
                    <th>IndustryTitle</th>
                </tr>
            </thead>
            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.Code)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.ADOffice)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.IndustryTitle)
                    </td>
                </tr>
            }
        </table>
    </div>
</body>
</html>
