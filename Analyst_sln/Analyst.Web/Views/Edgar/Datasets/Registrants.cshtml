@model IEnumerable<Analyst.Domain.Edgar.Registrant>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Registrants</title>
</head>
<body>
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.CIK)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CountryBA)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CityBA)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CountryInc)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EmployeeIdentificationNumber)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FilerStatus)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.WellKnownSeasonedIssuer)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FiscalYearEndDate)
            </th>
            <th></th>
        </tr>
    
    @foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.CIK)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CountryBA)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CityBA)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CountryInc)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EmployeeIdentificationNumber)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FilerStatus)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.WellKnownSeasonedIssuer)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FiscalYearEndDate)
            </td>
            <td>
                @Html.ActionLink("Details", "Details", new { id=item.Id })
            </td>
        </tr>
    }
    
    </table>
</body>
</html>
